{% extends 'base.html.twig' %}

{% block body %}
    <h1>Godz_przyj</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ godz_przyj.id }}</td>
            </tr>
            <tr>
                <th>Godzpoczatek</th>
                <td>{% if godz_przyj.godzPoczatek %}{{ godz_przyj.godzPoczatek|date('H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Godzkoniec</th>
                <td>{% if godz_przyj.godzKoniec %}{{ godz_przyj.godzKoniec|date('H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('godz_przyj_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('godz_przyj_edit', { 'id': godz_przyj.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
